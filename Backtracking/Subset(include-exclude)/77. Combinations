class Solution {
public:
    vector<vector<int>> res;

    void backtrack(int n, int k, vector<int>& path, int start) {
        if (k == 0) {
            res.push_back(path);
            return;
        }

        for (int i = start; i <= n; i++) {
            path.push_back(i);
            backtrack(n, k - 1, path, i + 1);
            path.pop_back();
        }
    }

    vector<vector<int>> combine(int n, int k) {
        vector<int> path;
        backtrack(n, k, path, 1);
        return res;
    }
};
